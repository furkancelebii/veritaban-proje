CREATE FUNCTION public.usergetir(prmt character varying) RETURNS TABLE(idsutun integer, kadisutun character varying, passutun character varying)
    LANGUAGE plpgsql
    AS $$
begin 
	return query
	select
	id,
	k_adi,
	pass
from
	users
where 
	k_adi like prmt;
end;
$$;


