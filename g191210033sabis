CREATE FUNCTION public.toplamitem() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
declare
	itemsayisi integer;
begin
itemsayisi:=(select length(item_name) from items order by id desc limit 1);
update toplamlar set toplamitem=toplamitem+itemsayisi;
return new;
end;
$$;


